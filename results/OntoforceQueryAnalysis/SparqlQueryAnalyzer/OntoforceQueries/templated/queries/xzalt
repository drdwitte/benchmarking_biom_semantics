PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX  dc: <http://purl.org/dc/elements/1.1/> PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX  dcterms: <http://purl.org/dc/terms/> PREFIX disq: <http://ns.ontoforce.com/2013/disqover#> SELECT ?ct ?label ?icon ?rank ?unclassified (COALESCE(?this_in_index, ?all_in_index) as ?in_index) ?defaultHidden ?description ?relevancyDate WHERE { GRAPH <http://ns.ontoforce.com/graphs/ontologies/integrator-u18> { ?ct disq:useAs disq:CanonicalType OPTIONAL { ?ct disq:icon ?icon } OPTIONAL { ?ct disq:rank ?rank } OPTIONAL { ?ct rdfs:label ?label } OPTIONAL { ?ct disq:inTextIndex ?this_in_index } OPTIONAL { disq:all disq:inTextIndex ?all_in_index } OPTIONAL { ?ct disq:unclassified ?unclassified } OPTIONAL { ?ct disq:description ?description } OPTIONAL { ?ct disq:defaultHidden ?defaultHidden } OPTIONAL { ?ct disq:relevancyDate ?relevancyDate }} } 
