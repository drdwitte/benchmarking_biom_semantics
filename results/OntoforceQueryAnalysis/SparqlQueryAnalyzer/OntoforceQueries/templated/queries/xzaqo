PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX  dc: <http://purl.org/dc/elements/1.1/> PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX  dcterms: <http://purl.org/dc/terms/> SELECT (COUNT(DISTINCT ?id) as ?total_count) WHERE { { SELECT ?original ?id WHERE { ?original <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. ?original_1 ?facet_source_1 ?facet_value_1. FILTER(?facet_source_1 = <http://www.w3.org/1999/02/22-rdf-syntax-ns#type>) FILTER (?facet_value_1 IN (<http://ns.ontoforce.com/ontologies/drugchemical/classes/meshDrugChemical>, <http://ns.ontoforce.com/ontologies/medicine/classes/substance>, <http://ns.ontoforce.com/ontologies/drugchemical/classes/cofactor>, <http://ns.ontoforce.com/ontologies/integration_ontology#DrugIntervention>, <http://rdf.ebi.ac.uk/terms/chembl#OligosaccharideTarget>, <http://ns.ontoforce.com/ontologies/drugchemical/classes/Compound>, <http://rdf.ebi.ac.uk/terms/chembl#SmallMoleculeTarget>, <http://rdf.ebi.ac.uk/terms/chembl#SmallMolecule>, <http://ns.ontoforce.com/ontologies/drugchemical/classes/Substance>, <http://ns.ontoforce.com/ontologies/drugchemical/classes/Drug>, <http://ns.ontoforce.com/ontologies/dailymed/Drug>)) ?original_2 ?facet_source_2 ?facet_value_2. FILTER(?facet_source_2 = <http://ns.ontoforce.com/ontologies/drugchemical/highestDevelopmentPhase>) FILTER(?facet_value_2 = <http://ns.ontoforce.com/ontologies/chembl/4>) OPTIONAL{ ?original_3 <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. { ?original_3 <http://ns.ontoforce.com/ontologies/drugchemical/group> ?facet_value_3 } } OPTIONAL{ ?original_4 <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. { ?original_4 <http://ns.ontoforce.com/ontologies/drugchemical/manufacturer> ?facet_value_4 } } ?original_1 <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. ?original_2 <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. FILTER(!BOUND(?facet_value_3)) FILTER(!BOUND(?facet_value_4)) } } UNION { SELECT ?original ?id WHERE { ?original ?facet_source_3 ?facet_value_5. FILTER(?facet_source_3 = <http://www.w3.org/1999/02/22-rdf-syntax-ns#type>) FILTER (?facet_value_5 IN (<http://ns.ontoforce.com/ontologies/drugchemical/classes/meshDrugChemical>, <http://ns.ontoforce.com/ontologies/medicine/classes/substance>, <http://ns.ontoforce.com/ontologies/drugchemical/classes/cofactor>, <http://ns.ontoforce.com/ontologies/integration_ontology#DrugIntervention>, <http://rdf.ebi.ac.uk/terms/chembl#OligosaccharideTarget>, <http://ns.ontoforce.com/ontologies/drugchemical/classes/Compound>, <http://rdf.ebi.ac.uk/terms/chembl#SmallMoleculeTarget>, <http://rdf.ebi.ac.uk/terms/chembl#SmallMolecule>, <http://ns.ontoforce.com/ontologies/drugchemical/classes/Substance>, <http://ns.ontoforce.com/ontologies/drugchemical/classes/Drug>, <http://ns.ontoforce.com/ontologies/dailymed/Drug>)) ?original ?facet_source_4 ?facet_value_6. FILTER(?facet_source_4 = <http://ns.ontoforce.com/ontologies/drugchemical/highestDevelopmentPhase>) FILTER(?facet_value_6 = <http://ns.ontoforce.com/ontologies/chembl/4>) OPTIONAL{ { ?original <http://ns.ontoforce.com/ontologies/drugchemical/group> ?facet_value_7 } } OPTIONAL{ { ?original <http://ns.ontoforce.com/ontologies/drugchemical/manufacturer> ?facet_value_8 } } OPTIONAL { ?original <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. } FILTER(!BOUND(?id)) FILTER(!BOUND(?facet_value_3)) FILTER(!BOUND(?facet_value_4)) FILTER(!BOUND(?facet_value_7)) FILTER(!BOUND(?facet_value_8)) } } } 
