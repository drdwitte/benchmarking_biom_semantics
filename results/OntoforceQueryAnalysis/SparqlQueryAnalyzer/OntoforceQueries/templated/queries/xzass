PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX  dc: <http://purl.org/dc/elements/1.1/> PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX  dcterms: <http://purl.org/dc/terms/> SELECT (COUNT(DISTINCT ?id) as ?total_count) WHERE { { SELECT ?original ?id WHERE { ?original <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. ?original_1 ?facet_source_1 ?facet_value_1. FILTER(?facet_source_1 = <http://www.w3.org/1999/02/22-rdf-syntax-ns#type>) FILTER (?facet_value_1 IN (<http://rdf.ebi.ac.uk/terms/chembl#Document>, <http://purl.org/spar/biro/BibliographicRecord>)) OPTIONAL{ ?original_2 <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. { ?original_2 <http://ns.ontoforce.com/ontologies/publication/contributor> ?facet_value_2 } } ?original_1 <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. FILTER(!BOUND(?facet_value_2)) } } UNION { SELECT ?original ?id WHERE { ?original ?facet_source_2 ?facet_value_3. FILTER(?facet_source_2 = <http://www.w3.org/1999/02/22-rdf-syntax-ns#type>) FILTER (?facet_value_3 IN (<http://rdf.ebi.ac.uk/terms/chembl#Document>, <http://purl.org/spar/biro/BibliographicRecord>)) OPTIONAL{ { ?original <http://ns.ontoforce.com/ontologies/publication/contributor> ?facet_value_4 } } OPTIONAL { ?original <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. } FILTER(!BOUND(?id)) FILTER(!BOUND(?facet_value_2)) FILTER(!BOUND(?facet_value_4)) } } } 
