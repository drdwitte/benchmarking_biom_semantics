PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX  dc: <http://purl.org/dc/elements/1.1/> PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX  dcterms: <http://purl.org/dc/terms/> PREFIX disq: <http://ns.ontoforce.com/2013/disqover#> SELECT ?property ?lens ?property_rank ?renderer ?prop_label ?facet_label { GRAPH <http://ns.ontoforce.com/graphs/uploaded_configuration_graph_20160224091238086725> { ?lens disq:showProperty ?property_description . FILTER(?lens = <http://ns.ontoforce.com/configuration#drugChemicalLens>) ?property_description disq:property ?property ; disq:rank ?property_rank . OPTIONAL { ?conf_facet a disq:Facet; disq:property ?property; rdfs:label ?facet_label . } OPTIONAL { ?property_description disq:renderer ?renderer } OPTIONAL { ?property a owl:DatatypeProperty . ?property rdfs:label ?prop_label } } } group by ?property ?lens ?property_rank ?renderer ?prop_label ?facet_label 
