PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX  dc: <http://purl.org/dc/elements/1.1/> PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX  dcterms: <http://purl.org/dc/terms/> SELECT ?val (coalesce(?label, ?val) AS ?val_label) ?vc WHERE { {SELECT ?val (COUNT(DISTINCT ?id) as ?vc) WHERE { { SELECT ?original ?id ?val WHERE { ?original <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. ?original_1 <http://vivoweb.org/ontology/core#affiliatedOrganization> ?val. ?original_2 ?facet_source_1 ?facet_value_1. FILTER(?facet_source_1 = <http://www.w3.org/1999/02/22-rdf-syntax-ns#type>) FILTER (?facet_value_1 IN (<http://rdf.ebi.ac.uk/terms/chembl#Document>, <http://purl.org/spar/biro/BibliographicRecord>)) ?original_1 <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. ?original_2 <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. } } UNION { SELECT ?original ?id ?val WHERE { ?original <http://vivoweb.org/ontology/core#affiliatedOrganization> ?val. ?original ?facet_source_2 ?facet_value_2. FILTER(?facet_source_2 = <http://www.w3.org/1999/02/22-rdf-syntax-ns#type>) FILTER (?facet_value_2 IN (<http://rdf.ebi.ac.uk/terms/chembl#Document>, <http://purl.org/spar/biro/BibliographicRecord>)) OPTIONAL { ?original <http://ns.ontoforce.com/2013/disqover#preferredUri> ?id. } FILTER(!BOUND(?id)) } } } GROUP BY ?val} OPTIONAL{ ?val <http://ns.ontoforce.com/2013/disqover#preferredLabel> ?label} } ORDER BY DESC(?vc) ?val_label LIMIT 18 OFFSET 0 
